/**app.wxss**/
page{
  background-color: #f5f5f5;
  height:100%;
  color: #333;
  font-size: 32rpx;
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.navigator{
  height: 120rpx;
  width: 100%;
  box-shadow: 0 -2px 5px #eaf2ff;
  background-color: white;
  position: fixed;
  bottom: 0;
  display: flex;
  align-items: center;
  justify-content: center;

}

.nav-bar{
  width: 31%;
  margin-left: 1%;
  margin-right: 1%;
  text-align: center;
  font-size: 32rpx;
  color: #8b8b8b;
  height: 100rpx;
  /* font-weight: bold; */
  display: flex;
  align-items: center;
  justify-content: center;
}
.active{
  color: #6eaeff;
  border-radius: 16rpx;
  background-color: #eaf2ff;

}